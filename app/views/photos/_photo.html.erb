<li id="photo-<%= photo.id %>">
  <span class="title"><%= "#{photo.title}" %></span>
  <span class="picture">
      <%= image_tag "/images/#{photo.picture}", size:'120x120', alt: "#{photo.picture}"%>
  </span>
  <span class="timestamp">
    Posted <%= time_ago_in_words(photo.created_at) %> ago.
  </span>
</li>